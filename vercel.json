{
  "version": 2,
  "builds": [
    {
      "src": "chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/chat.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
